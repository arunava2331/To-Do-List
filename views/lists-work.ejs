<%- include("header.ejs") -%> 
<body>
    <h1 class = "box" id = "heading"><%= kindOfTodo %></h1>

    
    <div class = "box">
        <% for(var i=0;i < newListItem.length;i++){ %>

            <div class = "item">
                <input type="checkbox">
                <p><%= newListItem[i] %></p>
            </div>
        <%} %>

        <form action="/work" method = "post">
            <input type="text" name="newItem" placeholder="New Item" autocomplete="off">
            <button type="submit" >+</button>
        </form>
    
</div>
</body>
</html>

<%-  include("footer.ejs") -%> 